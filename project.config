[access]
	inheritFrom = releng
[access "refs/*"]
	owner = group ldap/gerrit-releng-builder-committers
[access "refs/heads/*"]
	submit = deny group Project Owners
	label-Approved = -1..+1 group Project Owners
	label-Approved = -1..+0 group Registered Users
	label-Verified = -1..+2 group Non-Interactive Users
	label-Verified = -1..+1 group Administrators
	label-Verified = group Project Owners
[label "Approved"]
	function = MaxWithBlock
	abbreviation = A
	copyMinScore = true
	value = -1 Do not merge
	value =  0 No score
	value = +1 Conditionally merge
	defaultValue = 0
[label "Verified"]
	function = MaxWithBlock
	value = -1 Fails
	value =  0 No score
	value = +1 Verified
	value = +2 Verified and merge
	defaultValue = 0
[project]
